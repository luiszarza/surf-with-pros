<h1>Index</h1>


<div class="container py-5">

  <div class="row">
    <% @listings.each do |list| %>
      <div class="co-12 col-sm-6 col-md-4 mb-3">
        <%= link_to listing_path(list), class:"text-decoration-none" do %>
          <div class="card-category" data-aos="zoom-in" style="background-color:red">
            <%= list.title %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
